{
  "name": "moro/history-common",
  "description": "Package of common classes for feature 'entity history'.",
  "version": "0.2.0",
  "keywords": [
    "php",
    "dependency injection",
    "di",
    "container",
    "serializable",
    "psr-11"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Morozkin",
      "email": "andrey.dmitrievich@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Moro\\History\\Common\\": "sources/",
      "Moro\\History\\Test\\": "tests/sources/"
    }
  },
  "require": {
    "php": ">=7.1",
    "symfony/translation": "~4.0",
    "ext-json": "*"
  },
  "require-dev": {
    "codeception/codeception": "~2.3",
    "codeception/verify": "~1.0",
    "codeception/specify": "~1.0",
    "codeception/assert-throws": "~1.0",
    "infection/infection": "~0.5"
  },
  "archive": {
    "exclude": [
      "/vendor",
      "/tests",
      "/codeception.json",
      "/composer.lock",
      "/composer.phar",
      "/infection.json.dist",
      "/infection.php",
      "/phpunit.xml.dist",
      "/.gitattributes",
      "/.gitignore",
      "/.travis.yml"
    ]
  }
}
